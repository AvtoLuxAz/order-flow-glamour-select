
# Environment Configuration
# Copy this file to .env.local, .env.test, or .env.production depending on your environment

# Current environment: development, staging, or production
VITE_APP_ENV=staging

# API Configuration
VITE_API_URL=https://api.example.com
VITE_API_KEY=your_api_key_here

# Supabase configuration
VITE_SUPABASE_URL=https://your-supabase-project.supabase.co
VITE_SUPABASE_KEY=your-supabase-anon-key

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=true
VITE_USE_MOCK_DATA=true # Set to false to use real API/Supabase data

# App environment (local, staging, prod)
VITE_APP_MODE=local

# If APP_MODE is prod, you must provide the production API URL
VITE_PROD_API_URL=https://your-production-api.com
